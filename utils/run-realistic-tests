#!/bin/bash
set -e

java -jar lib/objectmacro.jar -d tests/objectmacro/intermediate/ tests/objectmacro/common/jpa_entity.objectmacro
java -jar lib/objectmacro-back.jar -d tests/objectmacro/java -p entity.macro tests/objectmacro/intermediate/jpa_entity.intermediate
javac -cp tests/objectmacro/java/ -d classes/ tests/objectmacro/java/entity/*.java
java -cp classes/ entity.Main
